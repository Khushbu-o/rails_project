<h2 class="textfont ">Login</h2>
<br>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :email ,:class => "title1"  %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email",:class =>"form-control1" %>
  </div>
<br>
  <div class="field">
    <%= f.label :password,:class => "title1"  %><br />
    <%= f.password_field :password, autocomplete: "current-password" ,:class =>"form-control1"%>
  </div>
<br>
  <% if devise_mapping.rememberable? %>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end %>

  <div class="actions">
    <%= f.submit "Log in",:class =>"btn" %>
  </div>
<% end %>
<%= render "devise/shared/links" %>
